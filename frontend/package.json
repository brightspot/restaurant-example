{
  "name": "restaurant-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen --require dotenv/config",
    "schema:backup": "node -e \"try{require('fs').copyFileSync('schema.graphql','schema.previous.graphql')}catch{}\"",
    "schema:diff": "graphql-inspector diff schema.previous.graphql schema.graphql",
    "codegen:diff": "yarn schema:backup && yarn codegen && yarn schema:diff"
  },
  "dependencies": {
    "antd": "^5.8.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@apollo/client": "^4.0.5",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/introspection": "^5.0.0",
    "@graphql-codegen/typed-document-node": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-inspector/cli": "^5.0.8",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "dotenv": "^17.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "graphql": "^16.11.0",
    "rxjs": "^7.8.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
